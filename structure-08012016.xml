<?xml version="1.0" encoding="windows-1251"?>
<!-- edited with XMLSpy v2007 sp2 (http://www.altova.com) by VI (GNIVC FNS RF) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:usch="http://www.unisoftware.ru/schematron-extensions" xmlns:sql="urn:schemas-microsoft-com:mapping-schema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="Файл">
		<xs:annotation>
			<!---*******************************************************************************************************************************************************************************-->
			<xs:appinfo>
				<sch:pattern name="Проверка имени файла на соответствие значению атрибута @ИдФайл">
					<sch:rule context="Файл">
						<!--  Проверка имени файла на соответствие ИдФайл                                                                             -->
						<sch:assert test="usch:getFileName() = @ИдФайл">
							<usch:error code="0400400007" path="@ИдФайл" value-of-select="@ИдФайл" doc-id-path="usch:getFileName()" doc-id-caption="Файл">Имя файла обмена "<usch:value-of select="usch:getFileName()"/>" не совпадает со значением элемента ИдФайл "<usch:value-of select="@ИдФайл"/>"</usch:error>
						</sch:assert>
					</sch:rule>
				</sch:pattern>
			</xs:appinfo>
			<!---*******************************************************************************************************************************************************************************-->
			<xs:documentation>Сведения из единого реестра субъектов малого и среднего предпринимательства</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ИдОтпр">
					<xs:annotation>
						<xs:documentation>Сведения об отправителе</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ФИООтв" type="ФИОТип">
								<xs:annotation>
									<xs:documentation>Фамилия, имя, отчество ответственного лица</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="ДолжОтв" use="optional">
							<xs:annotation>
								<xs:documentation>Должность ответственного лица</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="100"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="Тлф" use="optional">
							<xs:annotation>
								<xs:documentation>Номер контактного телефона</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="20"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="E-mail" use="optional">
							<xs:annotation>
								<xs:documentation>E-mail</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="45"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Документ" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Состав и структура документа</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:choice>
								<xs:element name="ОргВклМСП">
									<xs:annotation>
										<xs:documentation>Сведения о юридическом лице, включенном в реестр МСП</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:attribute name="НаимОрг" use="required">
											<xs:annotation>
												<xs:documentation>Полное наименование юридического лица на русском языке</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
													<xs:maxLength value="1000"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="НаимОргСокр" use="optional">
											<xs:annotation>
												<xs:documentation>Сокращенное наименование юридического лица на русском языке</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
													<xs:maxLength value="500"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="ИННЮЛ" type="ИННЮЛТип" use="required">
											<xs:annotation>
												<xs:documentation>ИНН юридического лица</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:complexType>
								</xs:element>
								<xs:element name="ИПВклМСП">
									<xs:annotation>
										<xs:documentation>Сведения об индивидуальном предпринимателе, включенном в реестр МСП</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="ФИОИП" type="ФИОТип">
												<xs:annotation>
													<xs:documentation>Фамилия, имя, отчество индивидуального предпринимателя</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
										<xs:attribute name="ИННФЛ" type="ИННФЛТип" use="required">
											<xs:annotation>
												<xs:documentation>ИНН индивидуального предпринимателя</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:complexType>
								</xs:element>
							</xs:choice>
							<xs:element name="СведМН">
								<xs:annotation>
									<!---*************************************************************************************************************************************************************-->
									<xs:appinfo>
										<sch:pattern name="Присутствие ФИО (руководителя организации / представителя)  в  зависимости  от  значения  ПрПодп и наличия НПЮЛ">
											<sch:rule context="СодПерСвед">
												<!-- Контроль  обязательности  присутствия  хотя бы одного вложенного элемента -->
												<sch:assert test=" count(СвПрод)!=0 or count(СвПрогПарт)!=0 or count(СвКонтр)!=0 or count(СвДог)!=0 ">
													<usch:error code="0400300000" path="СодПерСвед" value-of-select="count(СвПрод/*) "> Не выполнено условие присутствия хотя бы одного вложенного элемента в элементе Файл/Документ/СодПерСвед. </usch:error>
												</sch:assert>
											</sch:rule>
										</sch:pattern>
									</xs:appinfo>
									<!---*************************************************************************************************************************************************************-->
									<xs:documentation>Сведения о месте нахождения юридического лица / месте жительства индивидуального предпринимателя</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Регион" type="НаимАдрТип1">
											<xs:annotation>
												<xs:documentation>Субъект Российской Федерации</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Район" type="НаимАдрТип1" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Район (улус и т.п.)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Город" type="НаимАдрТип1" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Город (волость и т.п.)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="НаселПункт" type="НаимАдрТип2" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Населенный пункт (село и т.п.)</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="КодРегион" use="required">
										<xs:annotation>
											<xs:documentation>Код Региона</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="CCРФТип"/>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвОКВЭД" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Сведения о видах экономической деятельности по Общероссийскому классификатору видов экономической деятельности</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="СвОКВЭДОсн" type="СвОКВЭДТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения об основном виде деятельности</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвОКВЭДДоп" type="СвОКВЭДТип" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Сведения о дополнительных видах деятельности</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвЛиценз" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Сведения о лицензиях, выданных субъекту МСП</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="НаимЛицВД" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Наименование лицензируемого вида деятельности, на который выдана лицензия</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
													<xs:maxLength value="1000"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="СведАдрЛицВД" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Сведения об адресе места осуществления лицензируемого вида деятельности</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
													<xs:maxLength value="500"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="СерЛиценз" use="optional">
										<xs:annotation>
											<xs:documentation>Серия лицензии</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="10"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="НомЛиценз" use="required">
										<xs:annotation>
											<xs:documentation>Номер лицензии</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="100"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ВидЛиценз" use="optional">
										<xs:annotation>
											<xs:documentation>Вид лицензии</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="10"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ДатаЛиценз" type="ДатаТип" use="required">
										<xs:annotation>
											<xs:documentation>Дата лицензии</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ДатаНачЛиценз" type="ДатаТип" use="required">
										<xs:annotation>
											<xs:documentation>Дата начала действия лицензии</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ДатаКонЛиценз" type="ДатаТип" use="optional">
										<xs:annotation>
											<xs:documentation>Дата окончания действия лицензии</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ОргВыдЛиценз" use="optional">
										<xs:annotation>
											<xs:documentation>Наименование лицензирующего органа, выдавшего или переоформившего лицензию</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="500"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ДатаОстЛиценз" type="ДатаТип" use="optional">
										<xs:annotation>
											<xs:documentation>Дата приостановления действия лицензии</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ОргОстЛиценз" use="optional">
										<xs:annotation>
											<xs:documentation>Наименование лицензирующего органа, приостановившего действие лицензии</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="500"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвПрод" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Сведения о производимой субъектом МСП продукции</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="КодПрод" use="required">
										<xs:annotation>
											<xs:documentation>Код вида продукции</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="18"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="НаимПрод" use="required">
										<xs:annotation>
											<xs:documentation>Наименование вида продукции</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="1000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ПрОтнПрод" use="required">
										<xs:annotation>
											<xs:documentation>Признак отнесения продукции к инновационной, высокотехнологичной</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="1"/>
												<xs:enumeration value="1"/>
												<xs:enumeration value="2"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвПрогПарт" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Сведения о включении субъекта МСП в реестры программ партнерства</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="НаимЮЛ_ПП" use="required">
										<xs:annotation>
											<xs:documentation>Наименование заказчика, реализующего программу партнерства</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="1000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ИННЮЛ_ПП" type="ИННЮЛТип" use="required">
										<xs:annotation>
											<xs:documentation>ИНН заказчика, реализующего программу партнерства</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="НомДог" use="required">
										<xs:annotation>
											<xs:documentation>Номер договора о присоединении к выбранной программе партнерства</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="60"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ДатаДог" type="ДатаТип" use="required">
										<xs:annotation>
											<xs:documentation>Дата договора о присоединении к выбранной программе партнерства</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвКонтр" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Сведения о наличии у субъекта МСП в предшествующем календарном году контрактов, заключенных в соответствии с Федеральным законом от 5 апреля 2013 года №44-ФЗ</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="НаимЮЛ_ЗК" use="required">
										<xs:annotation>
											<xs:documentation>Наименование заказчика по контракту</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="1000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ИННЮЛ_ЗК" type="ИННЮЛТип" use="required">
										<xs:annotation>
											<xs:documentation>ИНН заказчика по контракту</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ПредмКонтр" use="optional">
										<xs:annotation>
											<xs:documentation>Предмет контракта</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="1000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="НомКонтрРеестр" use="required">
										<xs:annotation>
											<xs:documentation>Реестровый номер контракта</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="60"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ДатаКонтр" type="ДатаТип" use="optional">
										<xs:annotation>
											<xs:documentation>Дата заключения контракта</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвДог" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Сведения о наличии у субъекта МСП в предшествующем календарном году договоров, заключенных в соответствии с Федеральным законом от 18 июля 2011 года №223-ФЗ</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="НаимЮЛ_ЗД" use="required">
										<xs:annotation>
											<xs:documentation>Наименование заказчика по договору</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="1000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ИННЮЛ_ЗД" type="ИННЮЛТип" use="required">
										<xs:annotation>
											<xs:documentation>ИНН заказчика по договору</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ПредмДог" use="optional">
										<xs:annotation>
											<xs:documentation>Предмет договора</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="1000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="НомДогРеестр" use="required">
										<xs:annotation>
											<xs:documentation>Реестровый номер договора</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="60"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ДатаДог" type="ДатаТип" use="optional">
										<xs:annotation>
											<xs:documentation>Дата заключения договора</xs:documentation>
